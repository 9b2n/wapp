<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.mapper.RecommendedMapper">
    <insert id="insertRecommended">
        insert into recommended(lecNo)
        VALUES(#{lecNo})
    </insert>
    <delete id="deleteRecommended">
        delete from recommended where lecNo = #{lecNo}
    </delete>
</mapper>
